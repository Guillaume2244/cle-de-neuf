<!--=== Profile ===-->
<div class="profile container" >
  <div class="row content">
    <div class="col-md-12">

      <!--Profile Body-->
      <div class="profile-body">
        <!--Service Block v3-->
        <div class="row margin-bottom-10">
          <div class="col-sm-6 sm-margin-bottom-20">
            <div class="service-block-v3 servive-block-u">
              <i class="icon-users"></i>
              <span class="service-heading"></span>
              <span class="counter">Mon profil </span>


              <div class="row margin-bottom-20">
                <div class="col-xs-4 service-in statistics">
                  <h4 class="heading-xs">Adresse mail</h4>
                  <h4 class="heading-xs">Prénom</h4>
                  <h4 class="heading-xs">Nom</h4>
                  <h4 class="heading-xs">Téléphone</h4>

                </div>
                <div class="col-xs-6 text-left service-in">
                  <h4 class="counter"><%= current_user.email %></h4>
                  <h4 class="counter"><%= current_user.first_name %></h4>
                  <h4 class="counter"><%= current_user.last_name %></h4>
                  <h4 class="counter"><%= current_user.phone_number %></h4>
                </div>
              </div>
            </div>
          </div>


