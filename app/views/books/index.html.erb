      </div><!--/end row-->
      <!--End Service Block v3-->

      <div class="col-sm-12 sm-margin-bottom-20">
        <br>
        <div class="headline"><h2>Mes carnets d'entretien </div>
        <br>
        <div class="colorp"> <h3> <%= link_to 'Ajouter un véhicule', new_book_path, { :class=>"btn-u btn-u-9 pull-left" } %></h3> </div>
        <br>
        <!--Table Search v1-->
        <div class="table-search-v1 margin-bottom-20">
          <br>
          <div class="table-responsive">
            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Nom</th>
                  <th class="hidden-sm">Immatriculation</th>
                  <th> Date de mise en circulation</th>
                  <th>Kilométrage</th>
                  <th>Dernier entretien</th>
                  <th>Carnet</th>
                </tr>
              </thead>
              <tbody>
                <% @books.each do |book| %>
                <tr>

                  <td> <% if book.name %>
                    <%= book.name %>
                    <% end %> </td>
                    <td class="td-width"><%= book.registration_plate %></td>
                    <td><%if book.circulation_date %>
                      <%= book.circulation_date.strftime("%d/%m/%y") %>
                      <% end %></td>
                      <td><%= book.initial_km %>km.</td>
                      <td>
                        <% if book.checkups.last %>
                        <% if book.checkups.last.checkup_item %>
                        <%= book.checkups.last.checkup_item.name %> le
                        <%= book.checkups.last.checkup_item.created_at.strftime("%d/%m/%y") %>
                        <% end %>
                        <% end %>
                      </td>
                      <td> <%= link_to "Carnet ", book_path(book) , class: "btn-u btn-u-9 btn-u-xs" %> </i> </td>
                    </tr>
                    <%end%>
                  </tbody>
                </table>
              </div>
            </div>
            <!--End Table Search v1-->
          </div><!--/end row-->
        </div>
        <!--End Profile Body-->
      </div>
