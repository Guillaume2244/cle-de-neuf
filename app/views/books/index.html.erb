        -->
      </div><!--/end row-->
      <!--End Service Block v3-->



      <h2>Mes carnets d'entretien -
        <td><%= link_to "Ajouter un carnet d'entretien", new_book_path %></td></h2>

        <!--Table Search v1-->
        <div class="table-search-v1 margin-bottom-20">
          <div class="table-responsive">
            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Nom</th>
                  <th class="hidden-sm">Immatriculation</th>
                  <th> Date de mise en circulation</th>
                  <th>Kilométrage</th>
                  <th>Dernier entretien</th>
                  <th>Carnet</th>
                </tr>
              </thead>
              <tbody>
                <% @books.each do |book| %>
                <tr>
                  <td>
                    <a href="#">Titine</a>
                  </td>
                  <td class="td-width"><%= book.registration_plate %></td>
                  <td><%= book.circulation_date %></td>
                  <td><%= book.initial_km %>km.</td>
                  <td>
                    <% if book.checkups.last %>
                      <% if book.checkups.last.checkup_item %>
                        <%= book.checkups.last.checkup_item.name %> le
                        <%= book.checkups.last.checkup_item.created_at.strftime("%d/%m/%y") %>
                      <% end %>
                    <% end %>
                  </td>
                  <td><button <%= link_to "Carnet", book_path(book) ,:class=>"btn-u btn-u-9 btn-block btn-u-xs" %> <i class="fa fa-car margin-right-5"></i> </button></td>


                </tr>
                <%end%>
              </tbody>
            </table>
          </div>
        </div>
        <!--End Table Search v1-->
      </div><!--/end row-->
    </div>
    <!--End Profile Body-->
  </div>
</div><!--/end row-->
</div><!--/container-->
